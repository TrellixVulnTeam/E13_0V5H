{
  "name": "webpack-initial",
  "version": "1.0.0",
  "description": "webpack initial",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "json": "json-server --config json-server.json --watch db.json",
    "lint": "eslint src",
    "prepare": "husky install"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack"
  ],
  "author": "Alexey Pustovitin <apustovitin@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "csv-loader": "^3.0.3",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "mini-css-extract-plugin": "^2.2.2",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.3",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "core-js": "^3.17.2",
    "jquery": "^3.6.0",
    "json-server": "^0.16.3",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
